{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.exclude": {
    "**/dist/**": true,
    "**/node_modules/**": true,
    "**/.turbo/**": true,
    "**/.pnpm-store/**": true,
    "**/logs/**": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}
